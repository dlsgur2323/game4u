<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="Tag-Mapper">

	<select id="selectCategoList" resultType="tagkind">
		select * from gu_tagkind where category = '카테고리'
	</select>
	
	<select id="selectFuncList" resultType="tagkind">
		select * from gu_tagkind where category = '기능'
	</select>
	
	<select id="selectOSList" resultType="str">
		select * from gu_os
	</select>
	
</mapper>